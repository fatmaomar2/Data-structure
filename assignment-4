#include<iostream>
using namespace std;
class Node {
public:
    char data;
	Node* next;
};
int main() {
	Node* first = new Node();
	Node*second=new Node();
	first->data = 'A';
	first->next=second;
	second->data='B';
	
	cout << first->data << endl;
   cout << first->next->data<< endl;


	return 0;
}
